    "import yfinance as yf\n",
    "\n",
    "# Define the ticker symbol\n",
    "ticker_symbol = \"AAPL\"\n",
    "\n",
    "# Fetch the ticker data\n",
    "ticker = yf.Ticker(ticker_symbol)\n",
    "\n",
    "# Get option chain for specific expiration date\n",
    "opt = ticker.option_chain('2023-01-20')\n",
    "print(opt.calls)\n",
    "print(opt.puts)"
   ]
  }
